{
  "name": "nice-resume",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "lint-staged": {
    "*.{ts,tsx,md,html,css,json}": [
      "npm run lint --",
      "npm run format:check --",
      "npm run typescript --"
    ]
  },
  "scripts": {
    "dev": "vite",
    "typescript": "vue-tsc --noEmit",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint \"./src/**/*.{ts,vue}\"",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "prepare": "husky"
  },
  "dependencies": {
    "@heroicons/vue": "^2.1.1",
    "pinia": "^2.1.7",
    "vue": "^3.4.19"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "autoprefixer": "^10.4.18",
    "editorconfig": "^2.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.23.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vue-tsc": "^1.8.27"
  }
}
